# Core Configuration
NODE_ENV=development
PORT=3000

# OpenAI API
OPENAI_API_KEY=sk-your-api-key-here

# Security & Secrets (REQUIRED - generate secure random strings)
JWT_SECRET=your-jwt-secret-min-32-chars-required-here
WEBHOOK_SECRET=your-webhook-secret-here
MASTER_SECRET=your-master-secret-for-encryption-min-32-chars

# Qdrant Database
QDRANT_URL=http://qdrant:6333
QDRANT_API_KEY=

# Optional: Notification Services
TELEGRAM_BOT_TOKEN=
TELEGRAM_CHAT_ID=

DISCORD_WEBHOOK_URL=

# Logging
LOG_LEVEL=info

# Optional: Feature Flags
ENABLE_TELEMETRY=true
TRACK_QUERIES=true
